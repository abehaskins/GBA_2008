#ifndef GBA_KEYPAD_H
#define GBA_KEYPAD_H
#include "GBA_types.h"
#define KEY_A                  BIT0
#define KEY_B                  BIT1
#define KEY_SELECT             BIT2
#define KEY_START              BIT3
#define KEY_RIGHT              BIT4
#define KEY_LEFT               BIT5
#define KEY_UP                 BIT
#define KEY_DOWN               BIT7
#define KEY_R                  BIT8
#define KEY_L                  BIT9
#endif 
